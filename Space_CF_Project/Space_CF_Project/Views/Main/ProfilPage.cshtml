        <link rel="stylesheet" href="~/Tasarım_Dosyaları/Profil/css/bootstrap.css">

        <!-- Theme style  -->
        <link rel="stylesheet" href="~/Tasarım_Dosyaları/Profil/css/style.css">

        <!-- Modernizr JS -->
        <script src="~/Tasarım_Dosyaları/Profil/js/modernizr-2.6.2.min.js"></script>
