    <html>
    <body>

        <div class="fh5co-loader"></div>

        <div id="page">
            <header id="fh5co-header" class="fh5co-cover js-fullheight" role="banner" style="background-image: url(~/Tasarım_Dosyaları/Profil/images/.jpg);" data-stellar-background-ratio="0.5">
                <div class="overlay"></div>
                <div class="container">
                    <div class="row">
                        <div class="col-md-8 col-md-offset-2 text-center">
                            <div class="display-t js-fullheight">
                                <div class="display-tc js-fullheight animate-box" data-animate-effect="">
                                    <div class="profile-thumb"></div>
                                    <h1><span>PROFİL SAYFAM</span></h1>
                                    <p>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
        </div>
        <div id="fh5co-about" class="animate-box">
            <div class="container">
                <div class="row">
                    <div class="col-md-8 col-md-offset-2 text-center fh5co-heading">
