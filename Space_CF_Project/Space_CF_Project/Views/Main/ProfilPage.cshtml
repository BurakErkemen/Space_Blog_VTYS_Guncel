        <script src="~/Tasarım_Dosyaları/Profil/js/jquery.easing.1.3.js"></script>
        <!-- Bootstrap -->
        <script src="~/Tasarım_Dosyaları/Profil/js/bootstrap.min.js"></script>
        <!-- Waypoints -->
        <script src="~/Tasarım_Dosyaları/Profil/js/jquery.waypoints.min.js"></script>
        <!-- Stellar Parallax -->
        <script src="~/Tasarım_Dosyaları/Profil/js/jquery.stellar.min.js"></script>
